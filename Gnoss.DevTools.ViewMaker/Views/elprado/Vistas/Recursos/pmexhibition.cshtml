@functions
{
    public string EliminarCaracteresEspeciales(string pTexto)
    {
        return pTexto.Replace(" ", "-").Replace(".", "-").Replace("(", "-").Replace(")", "-").Replace(":", "-").Replace(",", "-").Replace("\"", "-").Replace("'", "-");
