@model RecentActivity

@*
Descripción
-----------------------------------------------
Vista que muestra la bandeja de las suscripciones de la comunidad.

Variables
----------------------------------------------------------------*@
@{ 
CommunityModel Comunidad = Html.GetComunidad();
UserProfileModel Perfil = Html.GetPerfil();
}

@*
----------------------------------------------------------------
-------------------- Vista de Bandeja de suscripciones --------------------
*@

@{
    Html.SetBodyClass("layout02 listadoMyGnoss listadoSuscripciones");
} 
  
<div class="row">
    @* Breadcrumb / Migas de pan en la ficha del recurso *@
    <div class="col col-12 col-breadcrumb" id="breadcrumb">
        <ul>
            <li><a href="@Comunidad.Url"><strong>@Html.GetText("COMMON", "HOME")</strong></a></li>
            <li>@Html.GetText("MENU", "SUSCRIPCIONES")</li>
        </ul>
    </div>

    <div class="col col-12 col-xl-12 col-contenido izquierda">
        <div class="wrapCol">
            <div class="header-contenido">
                <div class="h1-container">
                    <h1>@Html.GetText("MENU", "SUSCRIPCIONES")</h1>
                </div>
                @* Ocultar de momento la gestión de suscripciones (url)
                    <div class="col-xl-2">                   
                        <a class="btn btn-primary" href="@Perfil.Url@Html.GetText("URLSEM", "ADMINISTRARSUSCRIPCIONES")">@Html.GetText("SUSCRIPCIONES", "ADMINISTRARSUSCRIPCIONES")</a>
                    </div>
                *@
                <div class="resource-list listView resource-list-personas">
                    @Html.PartialView("ControlesMVC/_ActividadReciente", Model)
                </div>
            </div>
        </div>                
    </div>
</div>